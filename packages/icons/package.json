    "@types/react-dom": "^19.1.0",
    "react": "19.1.0",
    "react-dom": "19.1.0"
  },
  "devDependencies": {
    "@svgr/cli": "^8.1.0",
    "rimraf": "^5.0.5"
  },
  "dependencies": {
    "lucide-react": "^0.545.0"
  }
